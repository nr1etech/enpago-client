{
  "name": "@nr1etech/client",
  "description": "enpago client",
  "version": "0.1.0",
  "type": "module",
  "author": "NR1E, Inc",
  "publishConfig": {
    "access": "public"
  },
  "license": "BSD-3-Clause",
  "repository": {
    "type": "git",
    "url": "https://github.com/nr1etech/enpago-client.git"
  },
  "files": [
    "**"
  ],
  "scripts": {
    "build": "tsc",
    "prebuild": "prettier --check . && eslint src",
    "postbuild": "cp package.json README.md dist/ && pnpm run test",
    "watch": "tsc -w",
    "test": "vitest run --coverage",
    "clean": "rm -rf dist && rm -rf site && rm -rf coverage",
    "fmt": "prettier --write .",
    "site": "typedoc --out site \"src/**/*.ts\" --exclude \"src/**/*.test.ts\""
  },
  "devDependencies": {
    "@eslint/js": "^8.57.0",
    "@types/eslint__js": "^8.42.3",
    "typescript-eslint": "^7.16.0",
    "eslint": "^8.57.0",
    "prettier": "^3.3.2",
    "@types/cookie": "^0.6.0",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^22.9.0",
    "@vitest/coverage-v8": "^1.6.0",
    "tslib": "^2.6.3",
    "typedoc": "^0.25.13",
    "typescript": "~5.4.5",
    "vitest": "^1.6.0"
  },
  "dependencies": {
    "@aws-amplify/api-graphql": "^4.1.6",
    "cookie": "^0.6.0",
    "graphql": "^16.9.0",
    "valibot": "^0.35.0"
  },
  "peerDependencies": {
    "@authsure/flow-client": "^0.0.4",
    "@aws-amplify/api-graphql": "^4.1.6",
    "aws-amplify": "^6.3.6",
    "cookie": "^0.6.0"
  },
  "exports": {
    ".": {
      "import": "./index.mjs",
      "types": "./index.d.mts"
    },
    "./types": {
      "import": "./types/index.d.mts",
      "types": "./types/index.d.mts"
    }
  }
}
